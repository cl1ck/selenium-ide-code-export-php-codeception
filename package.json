  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:prod": "rm -rf build && env NODE_ENV=production yarn build",
    "test": "jest",
    "lint": "eslint webpack.config.babel.js src/ --ext .js --ext .jsx"
